[itemdef i_scroll_power_hour]
defname=i_scroll_power_hour
name=Power Hour Scroll
id=i_scroll_x_blue
type=t_script

on=@create
color=05d

on=@dclick
if (<topobj> != <src>)
  src.sysmessage You cannot use this where it is.
  return 1
endif
consume 1
if !(<src.findid.i_memory_power_hour>)
  serv.newitem i_memory_power_hour
  src.equip <new>
endif
if (<src.findid.i_memory_power_hour.more1> < 5)
  src.findid.i_memory_power_hour.more1 += 1
endif
src.findid.i_memory_power_hour.timer += 60*60
src.sysmessage You now have <eval (((<src.findid.i_memory_power_hour.timer>/60)/60)+1)> power hour(s) of 2x skillgain chance.
src.effect=3,i_fx_heal_effect,6,15,1
src.sound=snd_spell_bless
return 1

[itemdef i_memory_power_hour]
defname=i_memory_power_hour
name=Power Hour Memory
id=i_memory
type=t_eq_script

on=@equip
src.events=+e_power_hour

on=@timer
remove
return 1

on=@unequip
src.events=-e_power_hour
src.sysmessage Your power hour boost has expired.

[events e_power_hour]

on=@skillgain
argn2 *= 2

[itemdef i_scroll_bonus_loot_roll]
defname=i_scroll_bonus_loot_roll
name=Bonus Loot Roll Scroll
id=i_scroll_x_red
type=t_script

on=@create
color=021

on=@dclick
if (<topobj> != <src>)
  src.sysmessage You cannot use this where it is.
  return 1
endif
consume 1
if !(<src.findid.i_memory_bonus_loot_roll>)
  serv.newitem i_memory_bonus_loot_roll
  src.equip <new>
endif
src.findid.i_memory_bonus_loot_roll.more1 += 30
src.findid.i_memory_bonus_loot_roll.timer += 60*60
src.sysmessage You now have <src.findid.i_memory_bonus_loot_roll.dmore1> bonus loot rolls. Expiring in <eval (((<src.findid.i_memory_bonus_loot_roll.timer>/60)/60)+1)> hour(s).
src.effect=3,i_fx_heal_effect,6,15,1
src.sound=snd_spell_bless
return 1

[itemdef i_memory_bonus_loot_roll]
defname=i_memory_bonus_loot_roll
name=Bonus Loot Roll Memory
id=i_memory
type=t_eq_script

on=@timer
remove
return 1

on=@unequip 
src.sysmessage Your bonus loot rolls have expired.



