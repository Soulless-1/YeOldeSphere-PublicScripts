[typedef t_power_scroll]

on=@dclick
if (<topobj> != <src>)
  src.sysmessage You can't use this where it is.
  return 1
elseif (<src.<serv.skill.<more2>.name>> > <more1>)
  src.sysmessage Your <serv.skill.<more2>.name> is too high for this power scroll.
  return 1
//elseif (<more1> != 1050) && (<src.<serv.skill.<more2>.name>> < <eval (<more1>-50)>)
  //src.sysmessage You can't use this scroll yet.
  //return 1
endif
